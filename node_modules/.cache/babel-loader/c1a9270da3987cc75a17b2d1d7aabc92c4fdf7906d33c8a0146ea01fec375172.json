{"ast":null,"code":"import { ref, computed } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"HomePage\",\n  methods: {\n    async downloadData() {\n      try {\n        const response = await axios({\n          url: \"http://127.0.0.1:5000/download\",\n          method: \"GET\"\n        });\n      } catch (error) {\n        console.error(\"Error downloading data:\", error);\n        alert(\"Error downloading data: \" + error.message);\n      }\n    }\n  },\n  setup() {\n    const data = ref([]);\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\"http://127.0.0.1:5000/getData\");\n        data.value = response.data;\n      } catch (err) {\n        alert(\"Error fetching data: \" + err.message);\n      }\n    };\n    fetchData();\n    const filteredData = computed(() => {\n      return data.value.filter(part => part[\"Unnamed: 0\"] === \"Part No\");\n    });\n    const detail = computed(() => {\n      const partIndex = data.value.findIndex(row => row[\"Unnamed: 0\"] === \"Part No\");\n      return partIndex !== -1 ? data.value.slice(partIndex + 1) : [];\n    });\n    const tableHeaders = computed(() => {\n      return filteredData.value.length > 0 ? Object.keys(filteredData.value[0]).map(key => ({\n        text: filteredData.value[0][key],\n        value: key\n      })) : [];\n    });\n    console.log(filteredData);\n    return {\n      detail,\n      tableHeaders\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","axios","name","methods","downloadData","response","url","method","error","console","alert","message","setup","data","fetchData","get","value","err","filteredData","filter","part","detail","partIndex","findIndex","row","slice","tableHeaders","length","Object","keys","map","key","text","log"],"sources":["src/views/HomePage.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <div class=\"body\">\r\n      <div style=\"display: flex\">\r\n        <router-link to=\"/create\">\r\n          <v-btn\r\n            class=\"add\"\r\n            style=\"background-color: #0046f3; color: white; margin-right: 5px\"\r\n          >\r\n            <v-icon>mdi-plus</v-icon>\r\n            เพิ่มข้อมูล\r\n          </v-btn>\r\n        </router-link>\r\n        <v-btn class=\"edit\" style=\"background-color: #ffc300; color: white\">\r\n          <v-icon>mdi-pencil</v-icon>\r\n          แก้ไขข้อมูล\r\n        </v-btn>\r\n        <v-btn\r\n          class=\"add\"\r\n          style=\"background-color: #0046f3; color: white; margin-right: 5px\"\r\n          @click=\"downloadData\"\r\n        >\r\n          <v-icon>mdi-download</v-icon>\r\n          Download\r\n        </v-btn>\r\n      </div>\r\n      <div class=\"table-wrapper\">\r\n        <v-data-table :headers=\"tableHeaders\" :items=\"detail\">\r\n          <template v-slot:empty> </template>\r\n        </v-data-table>\r\n      </div>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from \"vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n  methods: {\r\n    async downloadData() {\r\n      try {\r\n        const response = await axios({\r\n          url: \"http://127.0.0.1:5000/download\",\r\n          method: \"GET\",\r\n        });\r\n\r\n      } catch (error) {\r\n        console.error(\"Error downloading data:\", error);\r\n        alert(\"Error downloading data: \" + error.message);\r\n      }\r\n    },\r\n  },\r\n\r\n  setup() {\r\n    const data = ref([]);\r\n\r\n    const fetchData = async () => {\r\n      try {\r\n        const response = await axios.get(\"http://127.0.0.1:5000/getData\");\r\n        data.value = response.data;\r\n      } catch (err) {\r\n        alert(\"Error fetching data: \" + err.message);\r\n      }\r\n    };\r\n\r\n    fetchData();\r\n\r\n    const filteredData = computed(() => {\r\n      return data.value.filter((part) => part[\"Unnamed: 0\"] === \"Part No\");\r\n    });\r\n\r\n    const detail = computed(() => {\r\n      const partIndex = data.value.findIndex(\r\n        (row) => row[\"Unnamed: 0\"] === \"Part No\"\r\n      );\r\n      return partIndex !== -1 ? data.value.slice(partIndex + 1) : [];\r\n    });\r\n\r\n    const tableHeaders = computed(() => {\r\n      return filteredData.value.length > 0\r\n        ? Object.keys(filteredData.value[0]).map((key) => ({\r\n            text: filteredData.value[0][key],\r\n            value: key,\r\n          }))\r\n        : [];\r\n    });\r\n    console.log(filteredData);\r\n    return {\r\n      detail,\r\n      tableHeaders,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.body {\r\n  display: grid;\r\n  padding: 20px;\r\n}\r\n\r\n.add,\r\n.edit {\r\n  display: flex;\r\n}\r\n.table-wrapper {\r\n  overflow-x: auto;\r\n  max-width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,SAAAA,GAAA,EAAAC,QAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,OAAA;IACA,MAAAC,aAAA;MACA;QACA,MAAAC,QAAA,SAAAJ,KAAA;UACAK,GAAA;UACAC,MAAA;QACA;MAEA,SAAAC,KAAA;QACAC,OAAA,CAAAD,KAAA,4BAAAA,KAAA;QACAE,KAAA,8BAAAF,KAAA,CAAAG,OAAA;MACA;IACA;EACA;EAEAC,MAAA;IACA,MAAAC,IAAA,GAAAd,GAAA;IAEA,MAAAe,SAAA,SAAAA,CAAA;MACA;QACA,MAAAT,QAAA,SAAAJ,KAAA,CAAAc,GAAA;QACAF,IAAA,CAAAG,KAAA,GAAAX,QAAA,CAAAQ,IAAA;MACA,SAAAI,GAAA;QACAP,KAAA,2BAAAO,GAAA,CAAAN,OAAA;MACA;IACA;IAEAG,SAAA;IAEA,MAAAI,YAAA,GAAAlB,QAAA;MACA,OAAAa,IAAA,CAAAG,KAAA,CAAAG,MAAA,CAAAC,IAAA,IAAAA,IAAA;IACA;IAEA,MAAAC,MAAA,GAAArB,QAAA;MACA,MAAAsB,SAAA,GAAAT,IAAA,CAAAG,KAAA,CAAAO,SAAA,CACAC,GAAA,IAAAA,GAAA,4BACA;MACA,OAAAF,SAAA,UAAAT,IAAA,CAAAG,KAAA,CAAAS,KAAA,CAAAH,SAAA;IACA;IAEA,MAAAI,YAAA,GAAA1B,QAAA;MACA,OAAAkB,YAAA,CAAAF,KAAA,CAAAW,MAAA,OACAC,MAAA,CAAAC,IAAA,CAAAX,YAAA,CAAAF,KAAA,KAAAc,GAAA,CAAAC,GAAA;QACAC,IAAA,EAAAd,YAAA,CAAAF,KAAA,IAAAe,GAAA;QACAf,KAAA,EAAAe;MACA,MACA;IACA;IACAtB,OAAA,CAAAwB,GAAA,CAAAf,YAAA;IACA;MACAG,MAAA;MACAK;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}