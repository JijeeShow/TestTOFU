{"ast":null,"code":"import { ref, computed } from \"vue\";\nimport axios from \"axios\";\nexport default {\n  name: \"HomePage\",\n  setup() {\n    const data = ref([]);\n    const loading = ref(true);\n    const error = ref(null);\n    const fetchData = async () => {\n      try {\n        const response = await axios.get(\"http://127.0.0.1:5000/getData\");\n        data.value = response.data;\n      } catch (err) {\n        error.value = \"Error fetching data: \" + err.message;\n      } finally {\n        loading.value = false;\n      }\n    };\n    fetchData();\n    const filteredData = computed(() => {\n      return data.value.filter(part => part[\"Unnamed: 0\"] === \"Part\");\n    });\n    const dataAfterPart = computed(() => {\n      const partIndex = data.value.findIndex(row => row[\"Unnamed: 0\"] === \"Part\");\n      return partIndex !== -1 ? data.value.slice(partIndex + 1) : [];\n    });\n    const tableHeaders = computed(() => {\n      return filteredData.value.length > 0 ? Object.keys(filteredData.value[0]).map(key => ({\n        text: filteredData.value[0][key],\n        value: key\n      })) : [];\n    });\n    return {\n      dataAfterPart,\n      loading,\n      tableHeaders\n    };\n  }\n};","map":{"version":3,"names":["ref","computed","axios","name","setup","data","loading","error","fetchData","response","get","value","err","message","filteredData","filter","part","dataAfterPart","partIndex","findIndex","row","slice","tableHeaders","length","Object","keys","map","key","text"],"sources":["src/views/HomePage.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <div class=\"body\">\r\n      <div style=\"display: flex\">\r\n        <router-link to=\"/create\">\r\n          <v-btn\r\n            class=\"add\"\r\n            style=\"background-color: #0046f3; color: white; margin-right: 5px\"\r\n          >\r\n            <v-icon>mdi-plus</v-icon>\r\n            เพิ่มข้อมูล\r\n          </v-btn>\r\n        </router-link>\r\n        <v-btn class=\"edit\" style=\"background-color: #ffc300; color: white\">\r\n          <v-icon>mdi-pencil</v-icon>\r\n          แก้ไขข้อมูล\r\n        </v-btn>\r\n      </div>\r\n      <div class=\"table-wrapper\">\r\n        <v-data-table\r\n          v-if=\"!loading\"\r\n          :headers=\"tableHeaders\"\r\n          :items=\"dataAfterPart\"\r\n        >\r\n          <template v-slot:empty>\r\n            <v-alert>No data available.</v-alert>\r\n          </template>\r\n        </v-data-table>\r\n        <v-spinner v-else />\r\n        <v-alert v-if=\"error\" type=\"error\">{{ error }}</v-alert>\r\n      </div>\r\n    </div>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed } from \"vue\";\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"HomePage\",\r\n\r\n  setup() {\r\n    const data = ref([]);\r\n    const loading = ref(true);\r\n    const error = ref(null);\r\n\r\n    const fetchData = async () => {\r\n      try {\r\n        const response = await axios.get(\"http://127.0.0.1:5000/getData\");\r\n        data.value = response.data;\r\n      } catch (err) {\r\n        error.value = \"Error fetching data: \" + err.message;\r\n      } finally {\r\n        loading.value = false;\r\n      }\r\n    };\r\n\r\n    fetchData();\r\n\r\n    const filteredData = computed(() => {\r\n      return data.value.filter((part) => part[\"Unnamed: 0\"] === \"Part\");\r\n    });\r\n\r\n    const dataAfterPart = computed(() => {\r\n      const partIndex = data.value.findIndex(\r\n        (row) => row[\"Unnamed: 0\"] === \"Part\"\r\n      );\r\n      return partIndex !== -1 ? data.value.slice(partIndex + 1) : [];\r\n    });\r\n\r\n    const tableHeaders = computed(() => {\r\n      return filteredData.value.length > 0\r\n        ? Object.keys(filteredData.value[0]).map((key) => ({\r\n            text: filteredData.value[0][key],\r\n            value: key,\r\n          }))\r\n        : [];\r\n    });\r\n\r\n    return {\r\n      dataAfterPart,\r\n      loading,\r\n      tableHeaders,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.body {\r\n  display: grid;\r\n  padding: 20px;\r\n}\r\n\r\n.add,\r\n.edit {\r\n  display: flex;\r\n}\r\n.table-wrapper {\r\n  overflow-x: auto;\r\n  max-width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AAoCA,SAAAA,GAAA,EAAAC,QAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EAEAC,MAAA;IACA,MAAAC,IAAA,GAAAL,GAAA;IACA,MAAAM,OAAA,GAAAN,GAAA;IACA,MAAAO,KAAA,GAAAP,GAAA;IAEA,MAAAQ,SAAA,SAAAA,CAAA;MACA;QACA,MAAAC,QAAA,SAAAP,KAAA,CAAAQ,GAAA;QACAL,IAAA,CAAAM,KAAA,GAAAF,QAAA,CAAAJ,IAAA;MACA,SAAAO,GAAA;QACAL,KAAA,CAAAI,KAAA,6BAAAC,GAAA,CAAAC,OAAA;MACA;QACAP,OAAA,CAAAK,KAAA;MACA;IACA;IAEAH,SAAA;IAEA,MAAAM,YAAA,GAAAb,QAAA;MACA,OAAAI,IAAA,CAAAM,KAAA,CAAAI,MAAA,CAAAC,IAAA,IAAAA,IAAA;IACA;IAEA,MAAAC,aAAA,GAAAhB,QAAA;MACA,MAAAiB,SAAA,GAAAb,IAAA,CAAAM,KAAA,CAAAQ,SAAA,CACAC,GAAA,IAAAA,GAAA,yBACA;MACA,OAAAF,SAAA,UAAAb,IAAA,CAAAM,KAAA,CAAAU,KAAA,CAAAH,SAAA;IACA;IAEA,MAAAI,YAAA,GAAArB,QAAA;MACA,OAAAa,YAAA,CAAAH,KAAA,CAAAY,MAAA,OACAC,MAAA,CAAAC,IAAA,CAAAX,YAAA,CAAAH,KAAA,KAAAe,GAAA,CAAAC,GAAA;QACAC,IAAA,EAAAd,YAAA,CAAAH,KAAA,IAAAgB,GAAA;QACAhB,KAAA,EAAAgB;MACA,MACA;IACA;IAEA;MACAV,aAAA;MACAX,OAAA;MACAgB;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}